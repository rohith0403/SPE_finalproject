{
  "name": "log4j",
  "version": "1.0.0",
  "description": "log2file",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "doc"
  ],
  "scripts": {
    "start": "nodemon --exec ts-node ./src/index.ts",
    "app": "nodemon --exec DEBUG=app:index node --inspect -r ts-node/register ./src/app.ts",
    "doc": "gulp typedoc",
    "docserver": "live-server ./doc --port=8088 ",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'tests/**/*.ts'",
    "prebuild": "rm -rf lib",
    "build": "tsc"
  },
  "author": "yun.zhao",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.7.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "debug": "^4.1.1",
    "gulp": "^4.0.2",
    "gulp-typedoc": "^2.2.2",
    "live-server": "^1.2.1",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.6.2"
  }
}
